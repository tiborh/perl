<!-- ========================================================================================== -->
<!-- Entity declarations                                                                        -->
<!-- ========================================================================================== -->

<!ENTITY % version              '"XSEIF R5"' >


<!-- ========================================================================================== -->
<!-- Namespace declarations                                                                     -->


<!ENTITY % namespace.att        'xmlns          CDATA           #FIXED
                                "urn:x-ericsson:r2:reg-doc:1551-fad.110.05:en:*"' >

<!ENTITY % xlink-namespace.att  'xmlns:xlink    CDATA           #FIXED
                                "http://www.w3.org/1999/xlink"' >


<!-- ========================================================================================== -->
<!-- Entities for Attributes                                                                    -->


<!ENTITY % common.att           "xml:id         ID              #IMPLIED
                                 xml:base       CDATA           #IMPLIED" >

<!ENTITY % role.att             'role           CDATA           #IMPLIED
                                 role-class     NMTOKEN         "local"' >

<!ENTITY % prof.att             'profile        NMTOKENS        #IMPLIED' >

<!ENTITY % compact.att          'compact        (yes | no) "no"' >

<!ENTITY % suppress.att         'suppress       (print | online | all | none) "none"' >

<!ENTITY % indent.att           'indent         (left-margin | half-indent | column) "column"' >

<!ENTITY % language.code.att    "code           (aa | ab | ae | af | am | ar | as | ay |
                                                 az | ba | be | bg | bh | bi | bn | bo |
                                                 br | ca | cf | co | cs | cy | da | de |
                                                 dz | el | en | eo | es | et | eu | fa |
                                                 fi | fj | fl | fo | fr | fy | ga | gd |
                                                 gl | gn | gu | ha | hi | hr | hu | hy |
                                                 ia | ie | ik | in | is | it | iw | ja |
                                                 ji | jw | ka | kk | kl | km | kn | ko |
                                                 ks | ku | ky | la | ln | lo | lt | lv |
                                                 mi | mg | mk | ml | mn | mo | mr | ms |
                                                 mt | my | na | ne | nl | no | oc | om |
                                                 or | pa | pb | pl | ps | pt | qu | rm |
                                                 rn | ro | rw | sa | sd | sg | sh | si |
                                                 sl | sm | so | sq | sr | ss | st | su |
                                                 sv | sw | ta | tg | te | th | ti | tk |
                                                 tl | tn | to | tr | ts | tt | tw | uk |
                                                 ur | uz | vi | vo | wo | x  | xh | xl |
                                                 yo | yy | zh | zt | zs | zu) #REQUIRED" >


<!-- ========================================================================================== -->
<!-- Block level entities                                                                       -->


<!ENTITY % block.component      "list | def-list | step-list | selection-list |
                                figure | table | example | input | output |
                                resource-id  | equation | block | np" >

<!ENTITY % list.component       "p | note | admonition | grid" >

<!ENTITY % list.content         "%list.component; | list" >

<!ENTITY % block.content        "%list.component; | %block.component;" >

<!ENTITY % p.inline             "emph | quote | subs | sups | literal | graphics |
                                eq-inline | object | keystroke | resource-id |
                                xref | link | ulink | wrap | preform | prod-no |
                                doc-no | input | output | nl | indexterm" >

<!ENTITY % title.group          "title, stitle?" >

<!ENTITY % caption.group        "caption, source?" >


<!-- ========================================================================================== -->
<!-- Element and Attribute declarations                                                         -->
<!-- ========================================================================================== -->

<!ELEMENT doc                   (meta-data, front?, body, back?) >
<!ATTLIST doc                   new-page-level  (npl0 | npl1 | npl2 | npl3 | npl4 | npl5) #IMPLIED
                                toc-level       (toc0 | toc1 | toc2 | toc3 | toc4 | toc5) "toc2"
                                list-of-figures (lof-yes | lof-no) "lof-no"
                                list-of-tables  (lot-yes | lot-no) "lot-no"
                                boxed-graphics  (bg-yes | bg-no) "bg-no"
                                boxed-examples  (be-yes | be-no) "be-no"
                                table-caption   (tc-yes | tc-no) "tc-yes"
                                math-support    (ms-yes | ms-no) "ms-no"
                                index-support   (is-yes | is-no) "is-no"
                                publication-no  (doc-id | part-of) "doc-id"
                                new-page-right  (npr-yes | npr-no) "npr-yes"
                                cover-back      (cb-content-yes | cb-content-no | cb-no ) "cb-no"
                                version         CDATA           #FIXED %version;
                                %common.att;
                                %namespace.att; >


<!-- ========================================================================================== -->
<!-- Meta-data Elements                                                                         -->


<!ELEMENT meta-data             (confidentiality, doc-name, doc-info?, doc-id,
                                company-id?, part-of?, title, subtitle?, stitle?,
                                english-title?, user-info?, (drafted-by, approved-by,
                                checked-by?, factual-supervisor?, translated-by?) ,
                                doc-variants?, revision-history?) >
<!ATTLIST meta-data             %common.att; >

<!ELEMENT confidentiality       EMPTY >
<!ATTLIST confidentiality       class           (public | ericsson-internal |
                                                ericsson-confidential ) "ericsson-internal"
                                %common.att; >

<!ELEMENT doc-name              (#PCDATA) >
<!ATTLIST doc-name              %common.att; >

<!ELEMENT doc-info              (#PCDATA) >
<!ATTLIST doc-info              %common.att; >

<!ELEMENT doc-id                (doc-no, language, rev, date) >
<!ATTLIST doc-id                %common.att; >

<!ELEMENT doc-no                (#PCDATA) >
<!ATTLIST doc-no                type            (registration | reference | other) "reference"
                                %prof.att;
                                %common.att; >

<!ELEMENT language              EMPTY >
<!ATTLIST language              %language.code.att;
                                %common.att; >

<!ELEMENT rev                   (#PCDATA) >
<!ATTLIST rev                   %common.att; >

<!ELEMENT date                  (y, m, d) >
<!ATTLIST date                  %common.att; >

<!ELEMENT y                     (#PCDATA) >
<!ATTLIST y                     %common.att; >

<!ELEMENT m                     (#PCDATA) >
<!ATTLIST m                     %common.att; >

<!ELEMENT d                     (#PCDATA) >
<!ATTLIST d                     %common.att; >

<!ELEMENT company-id            (business-unit, company-name, company-address?,
                                phone?, company-symbol?) >
<!ATTLIST company-id            %common.att; >

<!ELEMENT business-unit         (#PCDATA) >
<!ATTLIST business-unit         %common.att; >

<!ELEMENT company-name          (#PCDATA) >
<!ATTLIST company-name          %common.att; >

<!ELEMENT company-address       (line+) >
<!ATTLIST company-address       %common.att; >

<!ELEMENT company-symbol        (graphics?) >
<!ATTLIST company-symbol        logotype        (ericsson)      #IMPLIED
                                %common.att; >

<!ELEMENT part-of               (#PCDATA | span)* >
<!ATTLIST part-of               %common.att; >

<!ELEMENT title                 (#PCDATA | span | indexterm)* >
<!ATTLIST title                 %common.att; >

<!ELEMENT subtitle              (#PCDATA | span)* >
<!ATTLIST subtitle              %common.att; >

<!ELEMENT stitle                (#PCDATA | span)* >
<!ATTLIST stitle                %common.att; >

<!ELEMENT english-title         (#PCDATA | span)* >
<!ATTLIST english-title         %common.att; >

<!ELEMENT span                  (#PCDATA) >
<!ATTLIST span                  %role.att;
                                %prof.att;
                                %common.att; >

<!ELEMENT user-info             (#PCDATA) >
<!ATTLIST user-info             %common.att; >

<!ELEMENT drafted-by            (person+) >
<!ATTLIST drafted-by            %common.att; >

<!ELEMENT factual-supervisor    (person) >
<!ATTLIST factual-supervisor    responsible     (person | department) "department"
                                %common.att; >

<!ELEMENT checked-by            (person) >
<!ATTLIST checked-by            checked         (yes | no) "no"
                                %common.att; >

<!ELEMENT approved-by           (person) >
<!ATTLIST approved-by           approved        (yes | no) "no"
                                %common.att; >

<!ELEMENT translated-by         (person) >
<!ATTLIST translated-by         %common.att; >

<!ELEMENT person                (name, (signature | initials),
                                ((location, company, department) | ext-addr)?, phone?) >
<!ATTLIST person                %common.att; >

<!ELEMENT name                  (#PCDATA) >
<!ATTLIST name                  %common.att; >

<!ELEMENT signature             (#PCDATA) >
<!ATTLIST signature             %common.att; >

<!ELEMENT initials              (#PCDATA) >
<!ATTLIST initials              %common.att; >

<!ELEMENT location              (#PCDATA) >
<!ATTLIST location              %common.att; >

<!ELEMENT company               (#PCDATA) >
<!ATTLIST company               %common.att; >

<!ELEMENT department            (#PCDATA) >
<!ATTLIST department            %common.att; >

<!ELEMENT ext-addr              (line+) >
<!ATTLIST ext-addr              %common.att; >

<!ELEMENT line                  (#PCDATA) >
<!ATTLIST line                  %role.att;
                                %common.att; >

<!ELEMENT phone                 ((ecn-code?, extension) | public-no) >
<!ATTLIST phone                 %common.att; >

<!ELEMENT ecn-code              (#PCDATA) >
<!ATTLIST ecn-code              %common.att; >

<!ELEMENT extension             (#PCDATA) >
<!ATTLIST extension             %common.att; >

<!ELEMENT public-no             (#PCDATA) >
<!ATTLIST public-no             %common.att; >

<!ELEMENT doc-variants          (variant+) >
<!ATTLIST doc-variants          %common.att; >

<!ELEMENT variant               EMPTY >
<!ATTLIST variant               number          CDATA           #REQUIRED
                                name            CDATA           #IMPLIED
                                profile-names   NMTOKENS        #REQUIRED
                                %common.att; >

<!ELEMENT revision-history      (rev-info+) >
<!ATTLIST revision-history      suppress        (yes | no) "yes"
                                %common.att; >

<!ELEMENT rev-info              (rev, date, signature, description) >
<!ATTLIST rev-info              %common.att; >

<!ELEMENT description           (#PCDATA) >
<!ATTLIST description           %common.att; >


<!-- ========================================================================================== -->
<!-- Front Elements                                                                             -->


<!ELEMENT front                 ((dist-info | title-page)?,
                                enclosure-list?, abstract?, titled-block*) >
<!ATTLIST front                 %prof.att;
                                %common.att; >

<!ELEMENT dist-info             ((receivers | (present, not-present?)), info-copy?) >
<!ATTLIST dist-info             %prof.att;
                                %common.att; >

<!ELEMENT receivers             (person+) >
<!ATTLIST receivers             %common.att; >

<!ELEMENT present               (person+) >
<!ATTLIST present               %common.att; >

<!ELEMENT not-present           (person+) >
<!ATTLIST not-present           %common.att; >

<!ELEMENT info-copy             (person+) >
<!ATTLIST info-copy             %common.att; >

<!ELEMENT title-page            (graphics?, copyright, disclaimer*, trademark-list?) >
<!ATTLIST title-page            align-front-graphics (left | center | right) "right"
                                %prof.att;
                                %common.att; >

<!ELEMENT copyright             (p) >
<!ATTLIST copyright             %common.att; >

<!ELEMENT disclaimer            (p+) >
<!ATTLIST disclaimer            %prof.att;
                                %common.att; >

<!ELEMENT trademark-list        (tm-item+) >
<!ATTLIST trademark-list        %prof.att;
                                %common.att; >

<!ELEMENT tm-item               (trademark, owner) >
<!ATTLIST tm-item               %prof.att;
                                %common.att; >

<!ELEMENT trademark             (#PCDATA) >
<!ATTLIST trademark             %common.att; >

<!ELEMENT owner                 (#PCDATA) >
<!ATTLIST owner                 %common.att; >

<!ELEMENT enclosure-list        (en-item+) >
<!ATTLIST enclosure-list        %prof.att;
                                %common.att; >

<!ELEMENT en-item               (doc-no, title) >
<!ATTLIST en-item               %prof.att;
                                %common.att; >

<!ELEMENT abstract              (p+) >
<!ATTLIST abstract              %prof.att;
                                %common.att; >


<!-- ========================================================================================== -->
<!-- Body Elements                                                                              -->


<!ELEMENT body                  ((block?, titled-block*) | (part | chl1)*) >
<!ATTLIST body                  %common.att; >

<!ELEMENT block                 (%block.content;)* >
<!ATTLIST block                 %role.att;
                                %suppress.att;
                                %prof.att;
                                %common.att; >

<!ELEMENT titled-block          (title, (%block.content;)*) >
<!ATTLIST titled-block          %role.att;
                                %prof.att;
                                %common.att; >

<!ELEMENT part                  (title?, chl1*) >
<!ATTLIST part                  %role.att;
                                %prof.att;
                                %common.att; >

<!ELEMENT chl1                  (%title.group;, (%block.content;)*, titled-block*, chl2*) >
<!ATTLIST chl1                  %role.att;
                                %prof.att;
                                %common.att; >

<!ELEMENT chl2                  (%title.group;, (%block.content;)*, titled-block*, chl3*) >
<!ATTLIST chl2                  %role.att;
                                %prof.att;
                                %common.att; >

<!ELEMENT chl3                  (%title.group;, (%block.content;)*, titled-block*, chl4*) >
<!ATTLIST chl3                  %role.att;
                                %prof.att;
                                %common.att; >

<!ELEMENT chl4                  (%title.group;, (%block.content;)*, titled-block*, chl5*) >
<!ATTLIST chl4                  %role.att;
                                %prof.att;
                                %common.att; >

<!ELEMENT chl5                  (%title.group;, (%block.content;)*, titled-block*) >
<!ATTLIST chl5                  %role.att;
                                %prof.att;
                                %common.att; >


<!-- ========================================================================================== -->
<!-- Block level Elements                                                                       -->


<!ELEMENT admonition            (graphics?, p, p?) >
<!ATTLIST admonition            type            (warning | caution | danger | stop | do) #REQUIRED
                                %role.att;
                                %prof.att;
                                %common.att; >

<!ELEMENT grid                  (tgroup)+ >
<!ATTLIST grid                  frame           CDATA           #FIXED "none"
                                condensed       (yes | no) "no"
                                %indent.att;
                                %role.att;
                                %prof.att;
                                %common.att; >

<!ELEMENT p                     (#PCDATA | %p.inline;)* >
<!ATTLIST p                     annotation      CDATA           #IMPLIED
                                %role.att;
                                %prof.att;
                                %common.att; >

<!ELEMENT tp                    (#PCDATA | %p.inline; | tnote)* >
<!ATTLIST tp                    %role.att;
                                %prof.att;
                                %common.att; >

<!ELEMENT note                  (p | list)+ >
<!ATTLIST note                  %prof.att;
                                %common.att; >

<!ELEMENT tnote                 (#PCDATA | emph | quote | subs | sups | literal |
                                graphics | eq-inline | object | keystroke |
                                resource-id | xref | link | ulink | wrap)* >
<!ATTLIST tnote                 type            (general | source | specific) "specific"
                                %prof.att;
                                %common.att; >

<!ELEMENT figure                (graphics, %caption.group;) >
<!ATTLIST figure                align           (left | center | right) "left"
                                float           (yes | no) "no"
                                %indent.att;
                                %prof.att;
                                %common.att; >

<!ELEMENT example               (caption?, source?, preform) >
<!ATTLIST example               condensed       (yes | no) "no"
                                %role.att;
                                %indent.att;
                                %prof.att;
                                %common.att; >

<!ELEMENT caption               (#PCDATA | indexterm)* >
<!ATTLIST caption               %common.att; >

<!ELEMENT source                (#PCDATA) >
<!ATTLIST source                %common.att; >

<!ELEMENT preform               (#PCDATA | default | emph | input | output | indexterm)* >
<!ATTLIST preform               boxed           (yes | no)      #IMPLIED
                                %prof.att;
                                %common.att; >

<!ELEMENT default               (#PCDATA) >
<!ATTLIST default               %prof.att;
                                %common.att; >

<!ELEMENT input                 (#PCDATA | var | nl | indexterm)* >
<!ATTLIST input                 %prof.att;
                                %common.att; >

<!ELEMENT output                (#PCDATA | var | nl | indexterm)* >
<!ATTLIST output                %prof.att;
                                %common.att; >

<!ELEMENT resource-id           (#PCDATA | var | nl | indexterm)* >
<!ATTLIST resource-id           %role.att;
                                type            (uri | file-path | other) "file-path"
                                %prof.att;
                                %common.att; >

<!ELEMENT np                    EMPTY >
<!ATTLIST np                    %prof.att;
                                %common.att; >


<!-- ========================================================================================== -->
<!-- List block level Elements                                                                  -->


<!ELEMENT list                  (p?, list-item+, (bridge, list-item+)*) >
<!ATTLIST list                  type            (unordered | numeric | alphabetic) "unordered"
                                %compact.att;
                                %role.att;
                                %prof.att;
                                %common.att; >

<!ELEMENT list-item             (%list.content;)+ >
<!ATTLIST list-item             %compact.att;
                                %role.att;
                                %prof.att;
                                %common.att; >

<!ELEMENT selection-list        (p?, selection-item+, (bridge, selection-item+)*) >
<!ATTLIST selection-list        %compact.att;
                                %role.att;
                                %prof.att;
                                %common.att; >

<!ELEMENT selection-item        (%list.content; | selection-list)+ >
<!ATTLIST selection-item        selected        (checked | unchecked) "unchecked"
                                %compact.att;
                                %role.att;
                                %prof.att;
                                %common.att; >

<!ELEMENT def-list              (p?, dl-item+, (bridge, dl-item+)*) >
<!ATTLIST def-list              %role.att;
                                %prof.att;
                                %common.att; >

<!ELEMENT dl-item               ((term | graphics), def+) >
<!ATTLIST dl-item               %role.att;
                                %prof.att;
                                %common.att; >

<!ELEMENT term                  (#PCDATA | eq-inline | subs | sups | indexterm)* >
<!ATTLIST term                  %common.att; >

<!ELEMENT def                   (%list.content;)+ >
<!ATTLIST def                   %common.att; >

<!ELEMENT step-list             (p?, sl-item+, (bridge, sl-item+)*) >
<!ATTLIST step-list             reset           (yes | no) "yes"
                                %role.att;
                                %prof.att;
                                %common.att; >

<!ELEMENT sl-item               (%list.content; | def-list)+ >
<!ATTLIST sl-item               %compact.att;
                                %role.att;
                                %prof.att;
                                %common.att; >

<!ELEMENT bridge                (%list.component;)+ >
<!ATTLIST bridge                %prof.att;
                                %common.att; >

<!ELEMENT tbl-list              (tbl-item, tbl-item+) >
<!ATTLIST tbl-list              %prof.att;
                                %common.att; >

<!ELEMENT tbl-item              (tp) >
<!ATTLIST tbl-item              %prof.att;
                                %common.att; >


<!-- ========================================================================================== -->
<!-- Equation block level Elements                                                              -->


<!ELEMENT equation              (p?, fd, legend?, %caption.group;) >
<!ATTLIST equation              %indent.att;
                                %prof.att;
                                %common.att; >

<!ELEMENT legend                (legend-item+) >
<!ATTLIST legend                %prof.att;
                                %common.att; >

<!ELEMENT legend-item           (term, meaning) >
<!ATTLIST legend-item           %prof.att;
                                %common.att; >

<!ELEMENT meaning               (#PCDATA | subs | sups | eq-inline)* >
<!ATTLIST meaning               %common.att; >

<!ELEMENT eq-inline             (f) >
<!ATTLIST eq-inline             %prof.att;
                                %common.att; >


<!-- ========================================================================================== -->
<!-- Inline Elements                                                                            -->


<!ELEMENT graphics              EMPTY >
<!ATTLIST graphics              scalepct        CDATA           #IMPLIED
                                vertadjamt      CDATA           #IMPLIED
                                scalefit        (0 | 1) "0"
                                scaleheight     CDATA           #IMPLIED
                                scalewidth      CDATA           #IMPLIED
                                boxed           (yes | no)      #IMPLIED
                                resol           CDATA           #IMPLIED
                                xlink:type      CDATA           #FIXED "simple"
                                xlink:href      CDATA           #REQUIRED
                                xlink:show      CDATA           #FIXED "embed"
                                xlink:actuate   CDATA           #FIXED "onLoad"
                                %xlink-namespace.att;
                                %prof.att;
                                %common.att; >

<!ELEMENT emph                  (#PCDATA | subs | sups | xref | ulink | indexterm)* >
<!ATTLIST emph                  type            (low | medium | high) "medium"
                                %role.att;
                                %prof.att;
                                %common.att; >

<!ELEMENT quote                 (#PCDATA | subs | sups | emph | literal | input |
                                output | object | keystroke | resource-id |
                                graphics | xref | ulink | indexterm)* >
<!ATTLIST quote                 %role.att;
                                %prof.att;
                                %common.att; >

<!ELEMENT subs                  (#PCDATA | emph)* >
<!ATTLIST subs                  %role.att;
                                %prof.att;
                                %common.att; >

<!ELEMENT sups                  (#PCDATA | emph)* >
<!ATTLIST sups                  %role.att;
                                %prof.att;
                                %common.att; >

<!ELEMENT literal               (#PCDATA | indexterm)* >
<!ATTLIST literal               translatable    (yes | no) "no"
                                type            (text | code) "code"
                                %role.att;
                                %prof.att;
                                %common.att; >

<!ELEMENT object                (#PCDATA | indexterm)* >
<!ATTLIST object                %role.att;
                                %prof.att;
                                %common.att; >

<!ELEMENT prod-no               (#PCDATA) >
<!ATTLIST prod-no               %prof.att;
                                %common.att; >

<!ELEMENT keystroke             (#PCDATA) >
<!ATTLIST keystroke             %prof.att;
                                %common.att; >

<!ELEMENT var                   (#PCDATA) >
<!ATTLIST var                   %prof.att;
                                %common.att; >

<!ELEMENT wrap                  (#PCDATA | %p.inline;)* >
<!ATTLIST wrap                  %role.att;
                                %suppress.att;
                                %prof.att;
                                %common.att; >

<!ELEMENT nl                    EMPTY >
<!ATTLIST nl                    %prof.att;
                                %common.att; >


<!-- ========================================================================================== -->
<!-- Link inline Elements                                                                       -->


<!ELEMENT xref                  EMPTY >
<!ATTLIST xref                  linkend         IDREF           #REQUIRED
                                %prof.att;
                                %common.att;>

<!ELEMENT link                  (#PCDATA | subs | sups | emph | literal | input |
                                output | object | keystroke | resource-id |
                                graphics | xref | ulink | quote | wrap | indexterm)* >
<!ATTLIST link                  linkend         IDREF           #REQUIRED
                                %role.att;
                                %prof.att;
                                %common.att; >

<!ELEMENT ulink                 (#PCDATA | subs | sups | emph | literal | input |
                                output | object | keystroke | resource-id |
                                graphics | xref | link | quote | wrap | indexterm)* >
<!ATTLIST ulink                 xlink:type      CDATA           #FIXED "simple"
                                xlink:href      CDATA           #REQUIRED
                                xlink:title     CDATA           #IMPLIED
                                %xlink-namespace.att;
                                %prof.att;
                                %common.att; >


<!-- ========================================================================================== -->
<!-- Back Elements                                                                              -->


<!ELEMENT back                  (glossary?, reference?, index?) >
<!ATTLIST back                  %prof.att;
                                %common.att; >

<!ELEMENT glossary              (p?, glossary-list+) >
<!ATTLIST glossary              %prof.att;
                                %common.att; >

<!ELEMENT glossary-list         (gl-item+) >
<!ATTLIST glossary-list         %prof.att;
                                %common.att; >

<!ELEMENT gl-item               (term, expansion) >
<!ATTLIST gl-item               %prof.att;
                                %common.att; >

<!ELEMENT expansion             (p+) >
<!ATTLIST expansion             %common.att; >

<!ELEMENT reference             (p?, reference-list+) >
<!ATTLIST reference             %prof.att;
                                %common.att; >

<!ELEMENT reference-list        (rf-subsection, rf-item*) >
<!ATTLIST reference-list        %prof.att;
                                %common.att; >

<!ELEMENT rf-subsection         (#PCDATA) >
<!ATTLIST rf-subsection         %common.att; >

<!ELEMENT rf-item               (rf-title, subtitle?, doc-name?, doc-no?) >
<!ATTLIST rf-item               %prof.att;
                                %common.att; >

<!ELEMENT rf-title              (#PCDATA | xref | link | ulink)* >
<!ATTLIST rf-title              %common.att; >

<!ELEMENT index                 EMPTY >
<!ATTLIST index                 %prof.att;
                                %common.att; >


<!-- ========================================================================================== -->
<!-- Index Term Elements                                                                        -->


<!ELEMENT indexterm             (primary, (((secondary, ((tertiary, see?) | see)?)?) | see)?) >
<!ATTLIST indexterm             %prof.att; >

<!ELEMENT primary               (#PCDATA)* >
<!ATTLIST primary               sortas          CDATA           #IMPLIED >

<!ELEMENT secondary             (#PCDATA)* >
<!ATTLIST secondary             sortas          CDATA           #IMPLIED >

<!ELEMENT tertiary              (#PCDATA)* >
<!ATTLIST tertiary              sortas          CDATA           #IMPLIED >

<!ELEMENT see                   (#PCDATA)* >


<!-- ========================================================================================== -->
<!-- CALS Table Model declarations (based on the DocBk30 XML V1.0 DTD)                          -->
<!-- ========================================================================================== -->

<!-- ========================================================================================== -->
<!-- Entity declarations for Table                                                              -->


<!ENTITY % tbl.align.att        'align          (left | right | center | justify | char) #IMPLIED'>

<!ENTITY % tbl.valign.att       'valign         (top | middle | bottom) #IMPLIED' >


<!-- ========================================================================================== -->
<!-- Element and Attribute declarations for Table                                               -->


<!ELEMENT table                 (%caption.group;, (tgroup+)) >
<!ATTLIST table                 frame           (top | bottom | topbot | all |
                                                sides | none)   #IMPLIED
                                colsep          CDATA           #IMPLIED
                                rowsep          CDATA           #IMPLIED
                                orient          (port | land)   #IMPLIED
                                condensed       (yes | no) "no"
                                %indent.att;
                                %role.att;
                                %prof.att;
                                %common.att; >

<!ELEMENT tgroup                (colspec*, spanspec*, thead?, tfoot?, tbody) >
<!ATTLIST tgroup                cols            CDATA           #REQUIRED
                                tgroupstyle     CDATA           #IMPLIED
                                colsep          CDATA           #IMPLIED
                                rowsep          CDATA           #IMPLIED
                                %tbl.align.att;
                                char            CDATA           #IMPLIED
                                charoff         CDATA           #IMPLIED >

<!ELEMENT colspec               EMPTY >
<!ATTLIST colspec               colnum          CDATA           #IMPLIED
                                colname         CDATA           #IMPLIED
                                colwidth        CDATA           #IMPLIED
                                colsep          CDATA           #IMPLIED
                                rowsep          CDATA           #IMPLIED
                                %tbl.align.att;
                                char            CDATA           #IMPLIED
                                charoff         CDATA           #IMPLIED >

<!ELEMENT spanspec              EMPTY >
<!ATTLIST spanspec              namest          CDATA           #REQUIRED
                                nameend         CDATA           #REQUIRED
                                spanname        CDATA           #REQUIRED
                                colsep          CDATA           #IMPLIED
                                rowsep          CDATA           #IMPLIED
                                %tbl.align.att;
                                char            CDATA           #IMPLIED
                                charoff         CDATA           #IMPLIED >

<!ELEMENT thead                 (colspec*, row+) >
<!ATTLIST thead                 %tbl.valign.att; >

<!ELEMENT tfoot                 (colspec*, row+) >
<!ATTLIST tfoot                 %tbl.valign.att; >

<!ELEMENT tbody                 (row+) >
<!ATTLIST tbody                 %tbl.valign.att; >

<!ELEMENT row                   (entry)+ >
<!ATTLIST row                   rowsep          CDATA           #IMPLIED
                                %tbl.valign.att; >

<!ELEMENT entry                 (tp | tbl-list)* >
<!ATTLIST entry                 colname         CDATA           #IMPLIED
                                namest          CDATA           #IMPLIED
                                nameend         CDATA           #IMPLIED
                                spanname        CDATA           #IMPLIED
                                morerows        CDATA           #IMPLIED
                                colsep          CDATA           #IMPLIED
                                rowsep          CDATA           #IMPLIED
                                %tbl.align.att;
                                char            CDATA           #IMPLIED
                                charoff         CDATA           #IMPLIED
                                rotate          CDATA           #IMPLIED
                                %tbl.valign.att; >


